package integration.code_generation

class Arithmetic()

    let main(): void = do

        # Add
        println (1 + 2 + 5)
        println (1l + 2l + 5l)
        println (1.0f + 2.5f + 5.2f)
        println (1.0 + 2.5 + 5.2)

        # Subtract
        println (10 - 2 - 7)
        println (1l - 2l - 5l)
        println (1.0f - 2.5f - 5.2f)
        println (1.0 - 2.5 - 5.2)

        # Multiply
        println (2 * 4 * 5)
        println (1l * 2l * 5l)
        println (1.0f * 2.5f * 5.2f)
        println (1.0 * 2.5 * 5.2)

        # Divide
        println (10 / 5 / 2)
        println (1l / 2l / 5l)
        println (1.0f / 2.5f / 5.2f)
        println (1.0 / 2.5 / 5.2)

        # Combination
        println (1 * 2 / 3 + 4 - 2)
        println (1l * 2l / 5l - 3l + 1l)
        println (1.0f * 21.0f / 5.0f - 3.0f + 1.0f)
        println (1.0 * 2.0 / 5.1 - 3.1 + 4.2)

        # Parenthesis
        println (1 * 2 / (3 + 4) - 2)
        println (1l * 2l / (3l + 4l) - 2l)
        println (1.0f * 2.0f / (3.0f + 4.0f) - 2.0f)
        println (1.0 * 2.0 / (3.0 + 4.0) - 2.0)
